{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "MassTransit": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "Path": "log/Consumer.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 60
        }
      }
    ]
  },
  "ApiForwarding": {
    "BaseUrl": {
      "GDABApi": "https://localhost:7006",
      "TestApi": "https://api.example.com"
    },
    "AllPhotoOriginalAnalysisConsumer": { //Consumer 的Class 名稱
      "BaseUrlKey": "GDABApi",
      "Path": "/api/AllPhotoAnalysis/queue/photo-analysis/all-execution",
      "Method": "POST"
    },
    "PhotoPipelineExecutionConsumer": { //Consumer 的Class 名稱
      "BaseUrlKey": "GDABApi",
      "Path": "/api/PhotoPipeline/queue/photo-pipeline/execute",
      "Method": "POST"
    },
    "WrinkleAnalysisEvent": {
      "BaseUrlKey": "TestApi",
      "Path": "/analyze/wrinkle",
      "Method": "GET"
    },
    "MaskAnalysisEvent": {
      "BaseUrlKey": "TestApi",
      "Path": "/analyze/mask",
      "Method": "PUT"
    }
  }
}